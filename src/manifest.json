{
  "name": "Pocketbook",
  "options_page": "options.html",
  "short_name": "Pocketbook",
  "version": "0.1",
  "description": "A developer extension to store and run console scripts",
  "author": "Joseph Campuzano",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34-test.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "content_security_policy":
    "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": ["tabs", "<all_urls>"]
}
